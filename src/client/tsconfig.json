{
  "extends": "../../tsconfig.json",
  "include": [
    "**/*.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "#/*": [
        "../shared/*"
      ]
    },
    "module": "esnext"
  }
}
